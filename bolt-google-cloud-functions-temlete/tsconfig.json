{
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "jsx-slack",
        "esModuleInterop": true,
        "outDir": "./dist",
        "sourceMap": true,
        "strict": false,
        "noImplicitAny": false,
        "rootDir": "src",
        "module": "commonjs",
        "moduleResolution": "node",
        "types": [
            "node"
        ],
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "DOM",
            "ES6",
            "ScriptHost",
            "ES2015.Promise"
        ],
        "target": "es2017",
        "skipDefaultLibCheck": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
    },
    "compileOnSave": true,
}